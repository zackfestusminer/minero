<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
<appSettings>
  <!--
  30 = 0.5 min
  60 = 1 min
  720 = 12 min
  1800 = 30 min
  
  -->
      <add key="forNH" value="true"/>
  <!-- same for nh -->
    <add key="runFor" value="10"/>
    <add key="pauseFor" value="5"/>
    <add key="processName" value="notepad"/>

  <add key="ThreadConfig1Vega56" value="{ &quot;index&quot; : ReplaceThisIndex, &quot;intensity&quot; : 2016, &quot;worksize&quot; : 8, &quot;affine_to_cpu&quot; : false },"/>
  <add key="ThreadConfig2Vega56" value="{ &quot;index&quot; : ReplaceThisIndex, &quot;intensity&quot; : 1600, &quot;worksize&quot; : 8, &quot;affine_to_cpu&quot; : false },"/>

  <add key="ThreadConfig1NonVega" value="{ &quot;index&quot; : ReplaceThisIndex, &quot;intensity&quot; : 1000, &quot;worksize&quot; : 8, &quot;affine_to_cpu&quot; : false },"/>
  <add key="ThreadConfig2NonVega" value="{ &quot;index&quot; : ReplaceThisIndex, &quot;intensity&quot; : 1000, &quot;worksize&quot; : 8, &quot;affine_to_cpu&quot; : false },"/>

  <!-- 
  0 = no just 1
  1 = alternate 1 and 2
  2 = 2 always
  -->
  <add key="TwoThreadsForNonVegas" value="1"/>  
  
  
  <add key="PauseBetweenGPUResetandMining" value="20"/>  
  <add key="DisEnRXDevices" value="false"/>    
  <add key="VegaDeviceAliases" value="DEV_687F"/>
  <add key="RXDeviceAliases" value="DEV_67DF"/>
  <add key="DisEnPause" value="5"/>
  <add key="DisEnCycle" value="4"/>
  
   <add key="OD_Device0" value="Vega56"/>
   <add key="OD_Device1" value="RX580"/>
   <add key="OD_Device2" value="RX480"/>
   <add key="OD_Device3" value="RX580"/>
   <add key="OD_Device4" value="RX580"/>
   <add key="OD_Device5" value="RX580"/>
   <add key="OD_Device6" value="RX580"/>
   <add key="OD_Device7" value="RX580"/>
  


  </appSettings>
</configuration>
